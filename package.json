{
  "name": "react-study",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.8",
    "antd": "^5.21.3",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "normalize.css": "^8.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-quill": "^2.0.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2",
    "react-scripts": "5.0.1",
    "source-map-explorer": "^2.5.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "dotenv -e env/.env.development craco start",
    "start:prod": "dotenv -e env/.env.production craco start",
    "build": "dotenv -e env/.env.development craco build",
    "build:test": "dotenv -e env/.env.test craco build",
    "build:prod": "dotenv -e env/.env.production craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/*.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "dotenv-cli": "^7.4.2",
    "sass": "^1.79.4"
